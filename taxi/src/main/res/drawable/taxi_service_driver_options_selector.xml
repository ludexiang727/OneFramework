<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
  <item android:state_pressed="true">
    <shape
      android:shape="oval">
      <size android:width="25dp"
        android:height="25dp"/>
      <solid android:color="#f3f3f3"/>
    </shape>
  </item>

  <item>
    <shape
      android:shape="oval">
      <size android:width="25dp"
        android:height="25dp"/>
      <solid android:color="@android:color/transparent"/>
    </shape>
  </item>
</selector>